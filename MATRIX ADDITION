#include <stdio.h>

int main() {
    int i,j;
    int A[2][2]={{3,4},{5,6}};
    int B[2][2]={{7,8},{5,6}};
    int sum[2][2]={{0,0},{0,0}};
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++){
            sum[i][j]=A[i][j]+B[i][j];
        }
        printf("\n");
    }
    printf("matrix A is=\n");
       for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++){
            printf("%d",A[i][j]);
        }
        printf("\n");
    }
     printf("matrix B is=\n");
     for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++){
            printf("%d",B[i][j]);
        }
        printf("\n");
    }
     printf("matrix C is=\n");
     for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++){
            printf("%d",sum[i][j]);
        }
        printf("\n");
    }
    
    

    return 0;
}
OUTPUT:
matrix A is=
3 4
5 6
matrix B is=
7 8
5 6
matrix C is=
10 12
10 12
