#include <stdio.h>
int main() {
    int i,j;
    int A[2][2]={{10,11},{9,8}};
    int B[2][2]={{7,8},{5,6}};
    int sub[2][2]={{0,0},{0,0}};
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++){
            sub[i][j]=A[i][j]-B[i][j];
        }
        printf("\n");
    }
    printf("matrix A is=\n");
       for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++){
            printf("%d",A[i][j]);
        }
        printf("\n");
    }
     printf("matrix B is=\n");
     for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++){
            printf("%d",B[i][j]);
        }
        printf("\n");
    }
     printf("matrix sub is=\n");
     for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++){
            printf("%d",sub[i][j]);
        }
        printf("\n");
    }
    
    

    return 0;
}
OUTPUT:
matrix A is=
10 11
9  8
matrix B is=
7 8
5 6
matrix sub is=
3 3
4 2
